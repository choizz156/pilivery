{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"airbnb",
		"plugin:react/recommended",
		"plugin:prettier/recommended"
	],
	"rules": {
        "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/react-in-jsx-scope": "off",
        "no-unused-vars": 1,
        "react/prop-types": 0
    }
}
